pipeline {
  agent any
  stages {
    stage('Build') {
      steps {
        sh 'echo "Build stage is completed"'
      }
    }

    stage('Test') {
      steps {
        sh 'echo "Test is completed"'
      }
    }

    stage('Deploy') {
      steps {
        sh 'echo "deploy is completed"'
      }
    }

  }
}